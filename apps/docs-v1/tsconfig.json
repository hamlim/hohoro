{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "types": ["react/experimental", "./worker-configuration.d.ts", "bun-types"],
    "jsx": "react-jsx",
    "paths": {
      "#/*": ["./src/*"],
      "#/utils/*": ["./src/utils/*"]
    }
  }
}
